<script type="text/javascript"> // tabber
    $('#lot_id').keydown(function(e){
        if (e.keyCode == 13) { // barcode scanned!
            $('#qty').focus();
            return false; // block form from being submitted yet
        }
    });

    $('#qty').keydown(function(e){
      if ($('#qty').val() == '500'){
        
        if(e.keyCode == 13){
          $('#stack5_id1').focus();
          return false
        }
      } else if($('#qty').val() == '400'){
        
        if(e.keyCode == 13){
          $('#stack4_id1').focus();
          return false
        }
      } else if ($('#qty').val() == '300'){

        if(e.keyCode == 13){
          $('#stack3_id1').focus();
          return false
        }
      } else if ($('#qty').val() == '200'){

        if(e.keyCode == 13){
          $('#stack2_id1').focus();
          return false
        }
      } else if ($('#qty').val() == '100'){
        if(e.keyCode == 13){
          $('#stack1_id1').focus();
          return false
        }
      } else if ($('#qty').val() == '1') {
        if(e.keyCode == 13){
          $('#stack1_id1').focus();
          return false
        }
      }
    });

    //  stack 5
